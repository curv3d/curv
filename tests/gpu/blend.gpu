    |  vec2 r78 = vec2(r54,r77);
    |  float r79 = length(r78);
    |  float r80 = r79-r48;
    |  float r81 = min(r80,r54);
    |  float r82 = min(r81,r8);
    |  float r83 = min(r82,r34);
